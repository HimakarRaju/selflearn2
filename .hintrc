{
  "extends": [
    "development"
  ],
  "hints": {
    "apple-touch-icons": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "typescript-config/strict": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "@-moz-document",
          "scrollbar-width",
          "@-moz-document",
          "@-moz-document"
        ]
      }
    ]
  }
}